{
    "name": "fullscreeninteractive/silverstripe-azure-blob-storage",
    "type": "silverstripe-vendormodule",
    "description": "Adds Silverstripe Flysystem support for using the Azure Blob adapter",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "flysystem",
        "azure"
    ],
    "authors": [
        {
            "name": "Will Rossiter",
            "homepage": "https://www.fullscreen.io"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.8",
        "silverstripe/vendor-plugin": "^1.0",
        "league/flysystem-azure-blob-storage": "dev-drop-psr7 as 1.1.9",
        "league/flysystem-cached-adapter": "^1.1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/textagroup/flysystem-azure-blob-storage"
        }
    ],
    "autoload": {
        "psr-4": {
            "FullscreenInteractive\\SilverStripe\\AzureStorage\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
